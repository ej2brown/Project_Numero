  {
    "name": "Project Numero",
    "version": "1.0",
    "description": "Scraper helper",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "run_at": "document_idle",
            "js": ["jquery-3.4.1.min.js", "contentscript.js"]
        }
    ],
    "web_accessible_resources": ["injected.js"],
    "manifest_version": 2
  }